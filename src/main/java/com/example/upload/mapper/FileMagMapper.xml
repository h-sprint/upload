<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.upload.mapper.FileMagMapper">
    <insert id="insertFile" parameterType="com.example.upload.entity.FileMsg" useGeneratedKeys="true">
        INSERT INTO excel.filemsg (fileName, fileAddress, fileType)
        VALUES (#{fileName}, #{fileAddress}, #{fileType});
    </insert>
</mapper>